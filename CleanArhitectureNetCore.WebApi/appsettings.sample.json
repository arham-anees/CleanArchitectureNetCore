{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "clean.architecture.net5",
    "Audience": "clean.architecture.net5.audience"
  },
  "ExceptionLoggingPath": {
    "Activity": "../Logs\\Activity\\{0}\\activity.txt",
    "Error": "../Logs\\Errors\\{0}\\error.txt"
  },
  "ConnectionStrings": {
    "default": ""
  },
  /* enabling encryption will only accept encrypted values in request body */
  "enableEncryption": false
}
